{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "import-name": false,
    "no-boolean-literal-compare": false,
    "semicolon": [
      true,
      "always",
      "ignore-bound-class-methods"
    ],
    "no-console": [
      true,
      "log",
      "error"
    ]
  },
  "rulesDirectory": []
}
